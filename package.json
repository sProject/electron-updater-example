{
	"name": "app",
	"version": "0.9.74",
	"main": "main.js",
	"description": "electron-updater example project",
	"author": "Matt Haggard <e@mail.com>",
	"build": {
		"appId": "this.app.id",
		"publish": [
			{
				"provider": "s3",
				"bucket": "electron-bucket"
			}
		]
	},
