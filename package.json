{
	"name": "electron-updater-example",
	"version": "0.7.0",
	"main": "main.js",
	"description": "electron-updater example project",
	"author": "Matt Haggard",
	"build": {
		"appId": "com.github.sproject",
		"productName": "productName",
		"copyright": "Copyright Â© 2017 ${author}",
		"publish": ["github"],
		"mac": {
			"category": "your.app.category.type",
			"target": [
				"zip",
				"dmg"
			]
		},
		"linux": {
			"category": "your.app.category.type",
			"target": [
				"AppImage"
			]
		}
	},
	"scripts": {
		"start": "NODE_ENV=test electron .",
		"pub": "electron-builder --x64 -p always",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"devDependencies": {
		"electron": "^1.7.9",
		"electron-builder": "^19.43.5"
	},
	"dependencies": {
		"electron-log": "^2.2.11",
		"electron-updater": "^2.16.1"
	}
}
