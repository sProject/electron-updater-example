	"devDependencies": {
		"electron": "^1.7.9",
		"electron-builder": "^19.43.5",
		"electron-publisher-s3": "^19.46.5"
	},
	"dependencies": {
		"electron-log": "^2.2.11",
		"electron-updater": "^2.16.1"
	}
